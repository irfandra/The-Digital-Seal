# Docker Compose Environment Variables
# This file is used by docker-compose.yml for MySQL container only

# Database Configuration
# These credentials will be used to create the MySQL container
DB_USERNAME=root
DB_PASSWORD=password

# Note: Your Spring Boot .env file should match these credentials:
# DB_USERNAME=root
# DB_PASSWORD=password
# DATABASE_MODE=validate (or create for first time)
# 
# The Spring Boot app runs locally and connects to the MySQL container at localhost:3306
